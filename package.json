{
  "name": "interunit",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "dev:all": "turbo run dev:all",
    "lint:eslint": "eslint --ext .ts,.tsx .",
    "lint:tsc": "tsc",
    "preconstruct:build": "preconstruct build",
    "preconstruct:dev": "preconstruct dev",
    "create:package": "npx hygen create package",
    "util:clean": "rm -rf node_modules **/node_modules **/**/node_modules **/**/**/node_modules && rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean",
    "util:delete-dist": "rm -rf packages/**/dist",
    "postinstall": "preconstruct dev",
    "publish:all": "npm run util:delete-dist && preconstruct build && changeset version && changeset publish"
  },
  "dependencies":{
    "@preconstruct/cli": "^2.7.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.21.5",
    "@manypkg/cli": "^0.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interunit/ui.git"
  },
  "author": "Kyle McDonald",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/interunit/ui/issues"
  },
  "homepage": "https://github.com/interunit/ui#readme",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "@changesets/cli": "^2.26.2"
  }
}
