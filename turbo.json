{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["tsconfig.json", "package.json", "src/**"],
      "outputs": ["dist/**", "types/**"],
      "cache": false
    },
    "dev:all": {
      "cache": false,
      "persistent": true
    },
    "//#preconstruct:dev": {},
    "dev:docs": {
      "dependsOn": ["//#preconstruct:dev"]
    },
    "//#BARE:lint:tsc": {},
    "//#BARE:lint:eslint": {},
    "//#BARE:format:ci": {},
    "//#BARE:test:unit:ci": {},
    "lint:tsc": {
      "dependsOn": ["//#lint:tsc", "//#BARE:lint:tsc"]
    },
    "lint:eslint": {
      "dependsOn": ["//#lint:eslint", "//#BARE:lint:eslint"]
    },
    "format:ci": {
      "dependsOn": ["//#format:ci", "//#BARE:format:ci"]
    },
    "test:unit:ci": {
      "dependsOn": ["//#test:unit:ci", "//#BARE:test:unit:ci"]
    }
  }
}
