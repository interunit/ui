{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["tsconfig.json", "package.json", "src/**"],
      "outputs": ["dist/**", "types/**"],
      "cache": false
    },
    "lint": {
        "outputs": ["node_modules/.cache/.eslintcache"]
    },
    "lint:fix": {
        "outputs": ["node_modules/.cache/.eslintcache"]
    },
    "format": {
      "outputs": ["node_modules/.cache/.prettiercache"],
      "outputMode": "new-only"
    },
    "format:fix": {
      "outputMode": "new-only",
      "cache": false
    },
    "test:unit": {},
    "clean": {},
    "build:package:ci": {
        "outputs": ["dist/**"],
        "dependsOn": ["^build:package"]
    },
    "build:package": {
        "cache": false,
        "outputs": ["dist/**"],
        "dependsOn": ["^//#util:delete-dist", "^build:package"]
    },
    "publish:package": {
        "cache": false
    },
    "watch:package": {
        "cache": false,
        "persistent": true,
        "outputs": ["dist/**"],
        "dependsOn": ["^//#util:delete-dist", "^build:package"]
    },
    "dev:docs": {},
    "//#dev:packages": {},
    "//#util:delete-dist": {
        "cache": false
    },
    "//#build:packages": {}
  }
}
