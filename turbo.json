{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["tsconfig.json", "package.json", "src/**"],
      "outputs": ["dist/**", "types/**"],
      "cache": false
    },
    "dev:all": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "//#preconstruct:dev": {},
    "dev:docs": {
      "dependsOn": ["//#preconstruct:dev"]
    }
  }
}
